{
  "name": "apollo-link-timeout",
  "version": "5.0.0",
  "description": "Abort requests that take longer than a specified timeout period",
  "peerDependencies": {
    "@apollo/client": "^3.0.0"
  },
  "devDependencies": {
    "@apollo/client": "^3.1.3",
    "@types/jest": "^29.5.14",
    "@types/node": "^10.3.4",
    "graphql": "^15.3.0",
    "graphql-tag": "^2.11.0",
    "jest": "^29.7.0",
    "react": "^18.3.1",
    "ts-jest": "^29.2.5",
    "tslint": "^6.1.3",
    "typescript": "^5.6.3"
  },
  "type": "module",
  "types": "lib/timeoutLink.d.ts",
  "exports": {
    ".": {
      "import": "./lib/timeoutLink.js",
      "types": "./lib/timeoutLink.d.ts"
    }
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/drcallaway/apollo-link-timeout"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org/"
  },
  "scripts": {
    "build": "tsc",
    "lint": "tslint src/*.ts* src/**/*.ts*",
    "test": "jest",
    "prepack": "npm run build"
  },
  "keywords": [
    "graphql",
    "apollo",
    "apollo-link",
    "timeout"
  ],
  "author": "Dustin Callaway <drcallaway@gmail.com>",
  "maintainers": [
    "Tyler Scott <tylerscott@indeed.com>"
  ],
  "license": "MIT",
  "files": [
    "lib"
  ],
  "jest": {
    "transform": {
      ".(ts|tsx)": "ts-jest"
    },
    "testRegex": "(/__tests__/.*|\\.(test|spec))\\.(ts|tsx|js)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "json"
    ],
    "testPathIgnorePatterns": [
      "/node_modules/",
      "/lib/"
    ]
  }
}
